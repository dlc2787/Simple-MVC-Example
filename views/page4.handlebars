<html lang="en">
<head>
  <meta charset="utf-8"/>
  <title>{{title}}</title>
</head>
<body>
  <h1>Welcome to page4</h1>
  
  <!-- links or URLs are always GET resources as they are getting (retrieving) a resource -->
  <a href="/">Home</a>
    <a href="/page1">Page 1</a>
  <a href="/page2">Page 2</a>
  <a href="/page3">Page 3</a>
  <form action="/getName" method="GET">
  <input type="hidden" name="animal" value="dog">
  <input type="submit" value="Show Name">
  </form>

  {{#if dogs}}
    <ul>
      {{#each dogs}}
        <li> {{this.name}} is a {{this.breed}} dog and is {{this.age}} years old. </li>
      {{/each}}
    </ul>
  {{/if}}
</body>
</html>